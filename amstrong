class Amstrong{
    public static void main (String[] args)
    {
        
      int no=153;
        int sum=0;
        int i=no;
       while(no>0)
       {
        int rem=no%10;
        sum=sum+(rem*rem*rem);
        no=no/10;
       }
    
    if(sum==i){
        System.out.println("Amstrong number");
    }
    else
    {
        System.out.println("Not amstrong ");
    }
   }
}
