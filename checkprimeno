class Prime{
    public static void main(String [] args)
    {
        int f=0;
        int no=5;
        for(int i=2;i<=no/2;i++)
        {
            if(no%i==0)
            {
                f=1;
                break;
            }
        }
        if(f==0)
        {
            System.out.println("it is prime no");
        }
        else
        {
            System.out.println("it is not prime");
        }
    }
}
